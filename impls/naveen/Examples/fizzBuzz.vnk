(def! fizz-buzz
  (fn* (n)
    (if (= 0 (% n 15)) :fizz-buzz 
      (if (= 0 (% n 5)) :buzz 
        (if (= 0 (% n 3)) :fizz n)
      )
    )
  )
)

(prn (fizz-buzz 1))
